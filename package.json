{
  "name": "vitamin-d-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p start:mock-server next:dev",
    "next:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:mock-server": "prism mock openapi_1.yaml -p 3001",
    "prepare": "run-s codegen",
    "gen:openapi": "openapi-generator-cli generate -g typescript-fetch -i openapi_1.yaml -o src/api/generated",
    "clean:openapi": "rimraf src/api/generated",
    "codegen": "run-s clean:openapi gen:openapi"
  },
  "dependencies": {
    "@types/node": "20.2.6",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "firebase": "^9.22.2",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "rimraf": "^5.0.1"
  }
}
